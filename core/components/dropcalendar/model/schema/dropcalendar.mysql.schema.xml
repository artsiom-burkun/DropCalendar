<?xml version="1.0" encoding="UTF-8"?>
<model package="dropcalendar" baseClass="xPDOObject" platform="mysql" defaultEngine="MyISAM" phpdoc-package="dropcalendar"
       version="1.1">
    <object class="DropCalendarItem" table="dropcalendar_items" extends="xPDOSimpleObject">

        <field key="title"       dbtype="varchar" precision="100" phptype="text" null="false" default=""/>

        <field key="start"       dbtype="datetime"   phptype="datetime"  />
        <field key="end"         dbtype="datetime"   phptype="datetime"  />

        <field key="mesto"       dbtype="text" phptype="text" null="true" default=""/>
        <field key="prim"        dbtype="text" phptype="text" null="true" default=""/>
        <field key="className"   dbtype="text" phptype="text" null="true" default=""/>
        <field key="site"        dbtype="text" phptype="text" null="true" default=""/>

        <field key="calendar_id" dbtype="int" precision="10" phptype="integer" attributes="unsigned" null="true" default="0" />

    </object>

</model>